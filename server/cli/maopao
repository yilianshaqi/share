void maopao( int a[],int num)
{
	int i,j,temp;
	for(i=0;i<num-1,++i)
	{
		for(j=0;j<num-i-1;++j)
		{
			if(a[j]>a[j+1])
			{
				temp=a[j];
				a[j]=a[j+1];
				a[j+1]=temp;
			}
		}
	}
}
